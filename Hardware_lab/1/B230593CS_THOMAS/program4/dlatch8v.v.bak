module dlatch8(en,d,out,reset);
input en;
input[7:0] d;
output [7:0] out;
input reset;
dlatch d0 (en,d[0],out[0],reset);
dlatch d1 (en,d[1],out[1],reset);
dlatch d2 (en,d[2],out[2],reset);
dlatch d3 (en,d[3],out[3],reset);
dlatch d4 (en,d[4],out[4],reset);
dlatch d5 (en,d[5],out[5],reset);
dlatch d6 (en,d[6],out[6],reset);
dlatch d7 (en,d[7],out[7],reset);
endmodule
